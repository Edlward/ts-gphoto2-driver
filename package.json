{
  "name": "ts-gphoto2-driver",
  "version": "1.0.0",
  "description": "Node.js wrapper of C library libgphoto2 written in TypeScript",
  "main": "lib/index.js",
  "typings": "lib/index.d.js",
  "repository": "https://github.com/TypedProject/ts-gphoto2-driver.git",
  "author": "Romain Lenzotti",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "tslint": "tslint --project tsconfig.json",
    "test": "npm run clean && npm run tsc && npm run tslint && cross-env NODE_ENV=test nyc --reporter=html --reporter=text _mocha --recursive",
    "coveralls:travis": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "build": "tsc --project tsconfig.compile.json",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "dependencies": {
    "ffi-napi": "2.4.3",
    "ref": "^1.3.3",
    "ref-array": "^1.2.0",
    "ref-struct": "^1.1.0",
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/node": "^10.3.4",
    "@types/ref": "0.0.28",
    "@types/ref-array": "^0.0.28",
    "@types/ref-struct": "^0.0.29",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.5",
    "segfault-handler": "^1.0.1",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  }
}
